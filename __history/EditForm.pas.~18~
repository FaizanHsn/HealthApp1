{$R *.dfm}
uses
  Database;

procedure TForm3.OKBtnClick(Sender: TObject);
begin
  with Browse do
  begin
    try
      qMyfoodreceipes.Post;
    except
      On E: Exception do
      begin
        Exit;
      end;
    end;
    ModalResult := mrOK;
  end;

end;

end.
